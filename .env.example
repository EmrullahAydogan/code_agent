# Server Configuration
NODE_ENV=development
PORT=3001

# Database
DATABASE_PATH=./data/database.sqlite

# AI Provider API Keys
# At least one API key is required for the platform to function
# You can configure multiple providers and select them per agent

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key_here

# DeepSeek
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Anthropic Claude
CLAUDE_API_KEY=your_claude_api_key_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Project Storage
PROJECTS_PATH=./projects

# Logging
LOG_LEVEL=info

# Security
# Optional: Add authentication secret for multi-user setup
# JWT_SECRET=your_secret_key_here

# Rate Limiting (requests per minute)
RATE_LIMIT_PER_MINUTE=60

# Max file size for uploads (in bytes)
MAX_FILE_SIZE=10485760
